package com.nexsoft.looping;

public class SampleLooping5 {

	public static void main(String[] args) {
		int i=0, sum=0;
		while(i<5){
			i++;
			sum += i;
			if(i==2) break;
			System.out.print(i+ " \n");
		}
		//System.out.println("Totatlnya : "+sum);
		
		for(int i=0; i<10; i++) {
			if(i==4) break;
		}
		System.out.println(i);
	}
}
